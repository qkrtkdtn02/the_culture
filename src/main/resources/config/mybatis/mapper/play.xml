<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="p">
	<select id="rank_list" resultType="vo.PlayVO">
		Select p.*
		From play p inner join reple r 
		on p.play_id = r.play_id
		Order by r.reple_rating desc
	</select>
	
	<select id="genre_list" parameterType="String" resultType="vo.PlayVO">
		select * From play
		where play_genrenm = #{genre}
	
	</select>
</mapper>












