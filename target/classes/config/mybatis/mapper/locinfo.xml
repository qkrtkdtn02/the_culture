<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="l">
	<select parameterType="String" resultType="vo.LocInfoVO"
		id="loc_info">
		select * from locationinfo
		where loc_id = #{loc_id}
	</select>
</mapper>












